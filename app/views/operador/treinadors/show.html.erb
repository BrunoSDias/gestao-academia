<div class="page-header">
  <%= link_to operador_treinadors_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Voltar
  <% end %>
  <%= link_to edit_operador_treinador_path(@treinador), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Editar
  <% end %>
  <h1>Mostrar treinador</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt>
  <dd><%= @treinador.nome %></dd>

  <dt>Email:</dt>
  <dd><%= @treinador.email %></dd>

</dl>
<hr>

<%= link_to operador_treinador_treino_treinadors_path(@treinador), class: 'btn btn-primary' do %>
  <span class="glyphicon glyphicon-list"></span>
  Treinos
<% end %>
